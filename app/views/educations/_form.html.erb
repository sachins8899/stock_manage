		<div class="control-label col-md-3">
			<%= f.label :from%>
		</div>
		<div class="control-label col-md-2">
			<%=f.text_field :from_where, require: true, placeholder: "City"%>
		</div>
	</div>
	<div class="form-group">
		<div class="control-label col-md-1">
			<%= f.label :university%>
		</div>
		<div class="control-label col-md-2">
			<%=f.text_field :university, require: true, placeholder: "University or Board"%>
		</div>
		<div class="control-label col-md-3">
			<%= f.label :subjects%>
		</div>
		<div class="control-label col-md-2">
			<%=f.text_area :subjects, require: true, placeholder: "Subjects"%>
		</div>
	</div>